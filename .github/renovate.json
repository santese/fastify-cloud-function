{
    "extends": [
      "config:base",
      "group:allNonMajor",
      "schedule:earlyMondays",
      "packages:test",
      "packages:googleapis",
      "packages:linters"
    ],
    "packageRules": [{
        "matchPackagePatterns": ["eslint"],
        "groupName": "eslint"
      },
      {
        "matchPackagePatterns": ["axios"],
        "groupName": "axios"
      },
      {
        "matchPackagePatterns": ["prettier"],
        "groupName": "prettier"
      }
    ],
    "docker-compose": {
      "enabled": true
    }
  }
  